<p id="notice"><%= notice %></p>

<h1>Listing Ssds</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Select</th>
      <th>Price</th>
      <th>Name</th>
      <th>Model</th>
      <th>Form factor</th>
      <th>Size</th>
      <th>Connection</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ssds.each do |ssd| %>
      <tr>
	<td><%= ssd.select %></td>
        <td><%= ssd.price %></td>
        <td><%= ssd.name %></td>
        <td><%= ssd.model %></td>
        <td><%= ssd.form_factor %></td>
        <td><%= ssd.size %></td>
        <td><%= ssd.connection %></td>
        <td><%= link_to 'Show', ssd %></td>
        <td><%= link_to 'Edit', edit_ssd_path(ssd) %></td>
        <td><%= link_to 'Destroy', ssd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ssd', new_ssd_path %>
