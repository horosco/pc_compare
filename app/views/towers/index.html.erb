<p id="notice"><%= notice %></p>

<h1>Listing Towers</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Maker</th>
      <th>Model</th>
      <th>Mb compatibility</th>
      <th>Front ports</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @towers.each do |tower| %>
      <tr>
        <td><%= tower.price %></td>
        <td><%= tower.maker %></td>
        <td><%= tower.model %></td>
        <td><%= tower.mb_compatibility %></td>
        <td><%= tower.front_ports %></td>
        <td><%= link_to 'Show', tower %></td>
        <td><%= link_to 'Edit', edit_tower_path(tower) %></td>
        <td><%= link_to 'Destroy', tower, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tower', new_tower_path %>
